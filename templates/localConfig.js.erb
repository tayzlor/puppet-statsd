{
  backends: [<% @backends.each do |backend| -%>
  "<%= backend %>",
  <% end -%>]
, graphitePort: "<%= @graphiteport %>"
, graphiteHost: "<%= @graphiteserver %>"
, address: "<%= @address %>"
, port: "<%= @listenport %>"
, flushInterval: "<%= @flushinterval %>"
, percentThreshold: [<%= @percentthreshold.join(', ') %>]
, graphite: {
    globalPrefix: "stats.<%= @environment %>",
    globalSuffix: require('os').hostname().split('.')[0],
    legacyNamespace: false,
  }
<% @config.each do |k, v| -%>
, <%= k %>: <%= v %>
<% end -%>
}
