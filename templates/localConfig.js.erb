{
  backends: [<% @backends.each do |backend| -%>
  "<%= backend %>",
  <% end -%>]
, graphitePort: "<%= @graphiteport %>"
, graphiteHost: "<%= @graphiteserver %>"
, address: "<%= @address %>"
, port: "<%= @listenport %>"
, flushInterval: "<%= @flushinterval %>"
, percentThreshold: [<%= @percentthreshold.join(', ') %>]
<% if @graphite_options.length > 0 %->
, graphite: {
  <% @graphite_options.each do |k, v| -%>
    <%= k %>: <%= v %>,
  <% end -%>
}
<% end -%>
<% @config.each do |k, v| -%>
, <%= k %>: <%= v %>
<% end -%>
}
